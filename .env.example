# ========================================
# DietAI Beta 版环境变量配置
# ========================================
# 请复制此文件为 .env 并填写实际值

# ========================================
# AI 模型 API 密钥（必填其一）
# ========================================
# OpenAI API 密钥（推荐）
OPENAI_API_KEY=sk-your-openai-api-key-here

# Anthropic Claude API 密钥（可选）
ANTHROPIC_API_KEY=

# 阿里通义千问 API 密钥（可选）
DASHSCOPE_API_KEY=
DASHSCOPE_API_BASE=

# Groq API 密钥（可选）
GROQ_API_KEY=

# DeepSeek API（可选）
DEEPSEEK_API_BASE=

# ========================================
# 基础设施配置（Docker 环境已预配置）
# ========================================
# Redis 密码
REDIS_PASSWORD=123456

# ========================================
# 向量存储配置
# ========================================
VECTOR_STORE_PATH=agent/VectorStore
VECTOR_COLLECTION_NAME=nutrition_collection

# ========================================
# 其他 API（可选）
# ========================================
YUANJING_API_KEY=
YUNAJING_SECRET_KEY=
